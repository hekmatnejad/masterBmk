<?xml version="1.0" encoding="UTF-8"?>
<testSuite name="Trivial Driver case" xmlns="http://www.sun.com/japex/testSuite">

    <param name="japex.contextClassLoader" value="true"/>

    <param name="japex.resultUnit" value="ms"/>
    <param name="japex.warmupIterations" value="25"/>
    <param name="japex.runIterations" value="1"/>
    <param name="japex.maxStep" value="200"/>
    <param name="japex.engine" value="phreak"/>      <!-- rete or phreak -->
    <param name="japex.disableProfiler" value="false"/>
    <param name="japex.profilingTestCase" value="test2"/>

    <driver name="OpenCDS Native">
        <param name="japex.driverClass" value="opencds.benchmarking.phreak.BasicInsertMultiObject"/>
        <param name="description" value="OpenCDS Native Driver"/>
    </driver>
    <driver name="OpenCDS with Trait">
        <param name="japex.driverClass" value="opencds.benchmarking.phreak.BasicDonMultiObject"/>
        <param name="description" value="OpenCDS Trait Driver"/>
    </driver>

    <testCase name="test1"/>
